<p id="notice"><%= notice %></p>

<h1>Listing Scaffold2s</h1>

<table>
  <thead>
    <tr>
      <th>Flower</th>
      <th>Name</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scaffold2s.each do |scaffold2| %>
      <tr>
        <td><%= scaffold2.Flower %></td>
        <td><%= scaffold2.name %></td>
        <td><%= scaffold2.type %></td>
        <td><%= link_to 'Show', scaffold2 %></td>
        <td><%= link_to 'Edit', edit_scaffold2_path(scaffold2) %></td>
        <td><%= link_to 'Destroy', scaffold2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scaffold2', new_scaffold2_path %>
